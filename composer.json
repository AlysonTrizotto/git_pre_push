{
  "name": "alysontrizotto/git-pre-push",
  "description": "Biblioteca PHP para hooks de pre-push no Git.",
  "type": "composer-plugin",
  "license": "MIT",
  "keywords": ["git", "hook", "pre-push", "laravel", "phpunit", "composer-plugin"],
  "homepage": "https://github.com/alysontrizotto/git-pre-push",
  "support": {
    "issues": "https://github.com/alysontrizotto/git-pre-push/issues",
    "source": "https://github.com/alysontrizotto/git-pre-push"
  },
  "autoload": {
    "psr-4": {
      "GitPrePush\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "composer-plugin-api": "^2.0"
  },
  "require-dev": {
    "composer/composer": "^2.6",
    "phpunit/phpunit": "^9.6"
  },
  "bin": [
    "bin/git-pre-push"
  ],
  "extra": {
    "class": "GitPrePush\\Composer\\Plugin"
  }
}
